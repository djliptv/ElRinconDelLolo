  <extension point="xbmc.addon.metadata">
    <summary>Addon DJLIPTV</summary>
    <description lang="en">[COLOR white][B]GRATIS Y PARA TODOS[/B][/COLOR]</description>
    <platform>all</platform>
  </extension>
</addon>

